{
  "name": "csv-editor",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "cpx browser.js node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs",
    "ng": "ng",
    "start": "ng serve",
    "electron-start": "electron ./electron",
    "wait-on-npm-start": "wait-on http-get://localhost:4200 && npm run electron-start",
    "electron": "npm start & npm run wait-on-npm-start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clear": "rm -rf ./dist && rm -rf ./release",
    "ng-build": "ng build --prod --base-href . && cpx \"electron/*.{js,json}\" ./dist/CsvEditor",
    "electron-build:mac": "electron-packager ./dist/CsvEditor CsvEditor --platform=darwin --arch=x64 --out=./release --electronVersion=2.0.2 --icon=tools.icns  --ignore=node_modules/electron-* --overwrite",
    "electron-build:win32": "electron-packager ./dist/CsvEditor CsvEditor --platform=win32 --arch=x64 --out=./release --electronVersion=2.0.2 --icon=tools.ico  --ignore=node_modules/electron-* --overwrite",
    "electron-build:linux": "electron-packager ./dist/CsvEditor CsvEditor --platform=linux --arch=x64 --out=./release --electronVersion=2.0.2 --icon=tools.png  --ignore=node_modules/electron-* --overwrite",
    "build:mac": "npm run ng-build && npm run electron-build:mac",
    "build:win32": "npm run ng-build && npm run electron-build:win32",
    "build:linux": "npm run ng-build && npm run electron-build:linux"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@handsontable/angular": "^1.0.0-beta5",
    "core-js": "^2.5.4",
    "csvtojson": "^2.0.4",
    "electron": "^2.0.3",
    "handsontable": "^4.0.0",
    "hoek": "^5.0.3",
    "json2csv": "^4.1.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpx": "^1.5.0",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "wait-on": "^2.1.0",
    "webpack-electron-connect-plugin": "^1.0.4"
  }
}
